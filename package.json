    "react": "^16.6.3",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-md": "^1.9.2",
    "react-share": "^2.4.0",
    "react-slick": "^0.23.2",
    "react-twitter-widgets": "^1.7.1",
    "reactstrap": "^6.5.0",
    "slick-carousel": "^1.8.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "deploy": "gatsby build && gh-pages -d public -b gh-pages",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
